{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "/schemas/gym-schema",
	"type": "object",
	"required": [
		"@context",
		"credentialSubject"
	],
	"definitions": {
		"context": {
			"type": "array",
			"items": [{
				"const": "https://www.w3.org/2018/credentials/v1"
			}]
		},
		"credentialSubject": {
			"type": "object",
			"properties": {
				"Name": {
					"type": "string",
					"order": 0,
					"section": "Gym",
					"title": "Name"
				},
				"Age": {
					"type": "string",
					"order": 1,
					"section": "Gym",
					"title": "Age"
				},
				"Weight": {
					"type": "string",
					"order": 2,
					"section": "Gym",
					"title": "Weight"
				}
			}
		},
		"VerifiableCredential": {
			"const": "VerifiableCredential"
		}
	},
	"description": "GYM"
}
